{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "max-line-length": {
            "options": [ 90 ]
        }
    },
    "rules": {
        "max-line-length": {
            "options": [ 90 ]
        },
        "no-console": {
            "severity": "warning",
            "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
        },
        "quotemark": [
            true,
            "single"
        ],
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}