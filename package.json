{
  "name": "koa-req-validation",
  "version": "0.9.1",
  "description": "A request validation middleware for Koa.",
  "keywords": [
    "nodejs",
    "javascript",
    "typescript",
    "koa2",
    "validation"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm test && npm run build && npm run docs",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "demo": "./node_modules/.bin/ts-node ./demo/index.ts",
    "build": "./node_modules/.bin/tsc -p .",
    "docs": "./node_modules/.bin/typedoc --out docs/ src/ && touch docs/.nojekyll"
  },
  "author": "Perttu Kärnä <perttu.karna@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/ppeerttu/koa-req-validation.git"
  },
  "homepage": "https://ppeerttu.github.io/koa-req-validation/",
  "license": "ISC",
  "devDependencies": {
    "@koa/router": "^8.0.7",
    "@types/jest": "^25.1.2",
    "@types/koa": "^2.11.0",
    "@types/koa-bodyparser": "^4.2.2",
    "jest": "^25.1.0",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.0.3",
    "tslint": "^6.0.0",
    "typedoc": "^0.16.9",
    "typescript": "^3.3.3333",
    "validator": "^12.2.0"
  },
  "peerDependencies": {
    "@koa/router": "^8.0.x",
    "koa-bodyparser": "^4.2.x",
    "validator": "^12.2.x"
  },
  "dependencies": {
    "@types/koa__router": "^8.0.2",
    "@types/validator": "^12.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/*.ts",
      "src/**/*.ts"
    ]
  }
}
